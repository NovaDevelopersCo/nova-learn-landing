{
	"extends": [
		"next/core-web-vitals",
		"prettier"
	],
	"rules": {
		"import/no-default-export": "off",
		"no-console": "warn",
		"no-unused-vars": "error",
		"import/no-unresolved": "error"
	},
	"overrides": [
		{
			"files": [
				"src/components/**/*",
				"src/utils/**/*"
			],
			"rules": {
				"import/no-default-export": "error"
			}
		},
		{
			"files": [
				"src/types/**/*"
			],
			"rules": {
				"no-unused-vars": "off"
			}
		} // ignore params into type fn
	]
}